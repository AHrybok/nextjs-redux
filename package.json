{
  "name": "hello-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "now-build": "npm run build",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build ./web",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build ./web",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build ./web",
    "dev": "node server.js",
    "env": "node ./bin/env.js",
    "build": "next build ./web",
    "export": "rm -rf ./out && next export ./web && mv ./web/out ./out",
    "start": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production node server.js",
    "lint": "eslint 'web/**/*.js' 'tests/**/*.js' 'common/**/*.js'",
    "lint:fix": "eslint --fix 'web/**/*.js' 'common/**/*.js'",
    "test:e2e": "cross-env E2E=true nightwatch ./tests/e2e/index.e2e.js",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts ./tests/mocha.opts",
    "test:unit:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --watch --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:pact": "cross-env NODE_ENV=pactTest mocha --opts ./tests/mocha.opts \"**/*.pact.js\"",
    "test:pact:server": "cross-env MOCK_SERVER=true node_modules/.bin/nodemon --watch tests --exec node ./tests/pact/helpers/pact.mock-server.js",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js",
    "test:coverage": "istanbul cover ./web/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/register": "^7.0.0",
    "@pact-foundation/pact": "^7.2.0",
    "@welldone-software/why-did-you-render": "^3.3.3",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "10.0.1",
    "bootstrap": "4.1.3",
    "classnames": "2.2.6",
    "colors": "^1.3.3",
    "cookie": "^0.4.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "expect": "^24.9.0",
    "fs-extra": "^8.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "merge-deep": "^3.0.2",
    "mocha": "^6.2.0",
    "moment": "2.22.2",
    "next": "9.0.4",
    "next-offline": "^4.0.3",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "prettier": "^1.15.2",
    "rc-switch": "^1.9.0",
    "react": "^16.8.4",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.4",
    "react-maskedinput": "^4.0.1",
    "react-redux": "^5.1.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^6.1.1",
    "redux-saga": "^1.0.5",
    "webpack": "4.38.0"
  }
}
